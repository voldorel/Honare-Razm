    current_attack_anim = 0;
    wound  = false;
    pushed = false;
    stance = RUNWALK;
    scr_smooth_anim();
    timeline_running = false;
}
if stance = DASH {
    stance  = RUNWALK;
    h_speed = 0;
    scr_smooth_anim();
}
if stance = PARRIED {
    blocked = false;
    stance  = RUNWALK;
    h_speed = 0;
    scr_smooth_anim();
}
if stance = PARRY || stance = KICK{
    current_attack_anim = 0;
    parry               = false;
    blocked             = false;
    stance              = RUNWALK;
    scr_smooth_anim();
    timeline_running = false;
}
if stance = CUTSCENE {
    if talking = true {
        timeline_running = false;
        talking = false;
        current_anim = "stand";
        scr_smooth_anim();
    }
}
