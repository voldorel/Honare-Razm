            <string>///physics and animation
scr_anim_mix();

x += h_speed;


if h_speed != 0 { 
    if (move_dir = STAND) {
        if h_speed &gt; 0 {
            h_speed -= 1;
            if h_speed &lt; 0 
                h_speed = 0;
        }
        if h_speed &lt; 0 {
            h_speed += 1;
            if h_speed &gt; 0 
                h_speed = 0;
        }
    }
}
var t = id;
with obj_character {
    if t.stance != DEAD {
        if is_solid &amp;&amp; !cutscene {
            if t.is_solid &amp;&amp; !t.cutscene{
                if place_meeting(x, y, t) {
                    if stance != DEAD {
                        var xs = 1;
                        if x &gt; t.x
                            xs = 0;
                        move_contact_solid(xs * 180, 20);
                    }
                }
            }
        }
    }
}
with obj_block_wall {
    if t.is_solid &amp;&amp; !t.cutscene{
        if place_meeting(x, y, t) {
            var xs = 0;
            if x &gt; t.x
                xs = 1;
            with t
                move_contact_solid(xs * 180, 20);
        }
    }
}

