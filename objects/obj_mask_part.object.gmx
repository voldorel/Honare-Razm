            <string>///collision detection
var t   = id;
var f   = father;
var dir = 1;

if !instance_exists(f)//wrote this because of some fucking weird bug
    exit;

with (obj_kick_mask) {
    var t1     = id;
    if father.x &lt; f.x
        dir    = -1
    if (place_meeting(x, y, t)) {
        if father != f.id {
            with f {
                scr_pushback(dir, t);
                if !t1.hit
                    scr_play_sound(snd_kick);
            }
            hit = true;
        }
    }
}

with (obj_sword_mask_m) {
    var dmg           = damage;
    var t1            = id;
    var father_weapon = father.mainhand;
    if father.x &lt; f.x
        dir = -1
    if (place_meeting(x, y, t)) {
        if father != f.id {
            var ff  = father;
            var blk = false;
            var par = father.parry;
            if f.attack_mask != 0
                blk = true;
            with f {
                if par || blk {
                    var q = scr_is_attacking();
                    if q {
                        blocked = true;
                        wound   = false;
                        with instance_create(t1.x, t1.y, obj_spark) {
                            father = ff.part_fist;
                            scr_play_sound(father_weapon.parry_sound[irandom_range(0, 3)]);
                        }
                        if !par &amp;&amp; !wound
                            scr_pushback(dir, t);
                    }
                }
                else {
                    if !par {
                        scr_receive_damage(dmg, dir, t);
                        if (is_ai)
                            target = ff;
                        scr_play_sound(father_weapon.hit_sound[irandom_range(0, 3)]);
                        if scr_can_play_sound() &amp;&amp; !t1.hit {
                            if hhealth &gt; 0 {
                                audio_stop_sound(voice);
                                voice = scr_play_sound(wound_sound[irandom_range(0, 3)]);
                            }
                            else {
                                audio_stop_sound(voice);
                                if (!dead) {
                                    voice = scr_play_sound(death_sound[irandom_range(0, 3)]);   
                                    dead  = true;
                                }
                            }
                        }
                    }
                }
            }
            hit = true;
            if blk
                instance_destroy();
        }
    }
}
</string>
