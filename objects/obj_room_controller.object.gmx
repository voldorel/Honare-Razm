            <string>///draw game UI
var q       = obj_gamestats.HUD;
if (q) {
    var t   = 0, tt, hratio;
    t       = obj_player.hhealth;
    with obj_player {
        tt  = scr_max_health();
    }
    hratio  = 700 / tt;
    var xscale, xscale1;
    xscale  = t / sprite_get_width(spr_health_red);
    xscale *= hratio;
    if t &gt; fhealth
        fhealth = t;
    if t &lt; fhealth
        fhealth--;
        
    xscale1 = fhealth / sprite_get_width(spr_health_red);
    xscale1*= hratio;
    if !(cutscene) {
        draw_sprite(spr_healthbar, 0, 120, 75);
        draw_sprite_ext(spr_health_orange, 0, 142, 105, xscale1, 1, 0, -1, 1);
        draw_sprite_ext(spr_health_red, 0, 142, 105, xscale, 1, 0, -1, 1);
    }
    scr_draw_buttons();
}
scr_border();//draws black borders if cutscene is enabled
