            <string>event_inherited();
cloud_x = 0;


music_lowintense  = snd_demo_low_intensity;
music_highintense = snd_demo_high_intensity;
intense           = false;
level_music       = 0;

var name          = "sunny_map_state";
with obj_gamestats {
    scr_stat_entry(name, 0);
    var sstate    = scr_stat_get(name);
    var bar_state = 0;
    if scr_stat_entry_exists("bar_state")
        bar_state = scr_stat_get("bar_state");
    if scr_stat_entry_exists("house_state") {
        var hstate = scr_stat_get("house_state");
        if (hstate == 2 &amp;&amp; sstate &lt; 4) {
            scr_stat_update(name, 3);
        }
    }
    var sstate    = scr_stat_get(name);
    if (sstate == 0) {
        instance_create(3140, 720, obj_guard);
        scr_start_cutscene();
        with (obj_player) {
            scr_costume_black();
            image_xscale = -1;
        }
        instance_create(x, y, obj_cut_sun_0);
        scr_stat_update(name, 1);
    } else if (sstate == 1) {
        if (bar_state != 2) {
            instance_create(3140, 720, obj_guard);
            with (obj_player) {
                scr_costume_black();
                image_xscale = -1;
            }
        } else if (bar_state == 2) {
            scr_stat_update(name, 2);
            with obj_player {
                scr_costume_black();
                image_xscale = -1;
            }
            with (instance_create(2700, 720, obj_soldier)) {
                scr_costume_rebel_4();
            }
        }
    } else if (sstate == 2) {
        with obj_player {
            scr_costume_black();
            image_xscale = -1;
        }
        with (instance_create(2700, 720, obj_soldier)) {
            scr_costume_rebel_4();
        }
    } else if (sstate == 3) {
        with (obj_player) {
            scr_costume_black();
            skeleton_attachment_set("head", "char black/head_bearded");
            scr_set_weapon(obj_greatsword);
            image_xscale = 1;
        }
        with (instance_create(3500, 720, obj_soldier)) {
            image_xscale = -1;
            scr_costume_rebel_1();
            scr_set_weapon(obj_r_knight_sword);
            stand_anim = "stand";
        }
        instance_create(x, y, obj_cut_sun_1);
    }
    else if (sstate == 4) {
        scr_save_room_state(name, 4);
        instance_create(x, y, obj_cut_sun_2);
        with (obj_player) {
            x = 3500;
            scr_costume_black();
            skeleton_attachment_set("head", "char black/head_bearded");
            scr_set_weapon(obj_greatsword);
            image_xscale = 1;
        }
        with (instance_create(6000, 720, obj_soldier)) {
            image_xscale = -1;
            scr_costume_rebel_4();
            scr_set_weapon(obj_whitchsword);
            target = obj_player.id;
        }
        alarm[5] = 1;
    }
}
</string>
