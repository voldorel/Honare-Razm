            <string>///fade black
if !fadeout
    alpha += 0.05;
else 
    alpha -= 0.05;
if (alpha &gt; 1) {
    alpha = 1;
    if !counting {
        counting = true;
        alarm[0] = 3 * room_speed;
        room_goto(target_room);
    }
}
if (alpha &lt; 0) {
    alpha = 0;
    instance_destroy();
}

var t = (1 - alpha * 0.14);
var hh = hview * t;
var ww = wview * t;
if fadeout {
    view_wview[0] = ww;
    view_hview[0] = hh;
}


var tt = 1;

if fadeout
    tt = 1/t;
draw_set_color(c_black);
draw_set_alpha(alpha);
draw_set_font(font_menu);
draw_rectangle(-1, -1, ww * 2 + 1, hh * 2 + 1, 0);
if !blank {
draw_sprite(spr_gamelogo, 0, view_wview[0]/2 * tt, view_hview[0]* 0.42 * tt);
draw_set_color(c_white);
draw_set_halign(fa_center);
if (contains == 2)
