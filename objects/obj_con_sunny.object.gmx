            <kind>1</kind>
            <string>///init room and cutscenes
event_inherited();
cloud_x = 0;


music_lowintense  = snd_demo_low_intensity;
music_highintense = snd_demo_high_intensity;
intense           = false;
level_music       = 0;
music_enable      = true;
var t             = id;



var name          = "sunny2";
with obj_gamestats {
    var bar_state = 0;
    if scr_stat_entry_exists("bar_state")
        bar_state = scr_stat_get("bar_state");
    scr_stat_entry(name, 0);
    var sstate = scr_stat_get(name);
    if scr_stat_entry_exists("house_state") {
        var hstate = scr_stat_get("house_state");
        if (hstate == 2 &amp;&amp; sstate &lt; 2) {
            scr_stat_update(name, 2);
        }
    }
    sstate = scr_stat_get(name);
    if (sstate == 0) {
        with (obj_player) {
            scr_costume_black();
            image_xscale = -1;
        }
        if (bar_state == 2) {
            scr_save_room_state(name, 1);
            scr_stat_update(name, 1);
            instance_create(x, y, obj_cut_sd_0);
            with (instance_create(3800, 720, obj_soldier)) {
                scr_costume_rebel_2();
            }
        }else {
            with instance_create(3230, 800, obj_kid) {
                image_xscale = 1.2;
                image_yscale = 1.2;
            }
            instance_create(x, y, obj_cut_sd_1);
        }
    } else if (sstate == 1) {
        scr_stat_update(name, 1);
        scr_save_room_state(name, 1);
        with (obj_player) {
            scr_costume_black();
            image_xscale = -1;
        }
        instance_create(x, y, obj_cut_sd_0);
        with (instance_create(3800, 720, obj_soldier)) {
            scr_costume_rebel_2();
        }
    } else if (sstate == 2) {
        scr_save_room_state(name, 2);
        t.music_enable = false;
        with (obj_player) {
            scr_costume_black();
            skeleton_attachment_set("backpack", "char black/broadsword");
            skeleton_attachment_set("mainhand", "none");
            skeleton_attachment_set("head", "char black/head_bearded");
        }
        scr_stat_update(name, 3);
        instance_create(x, y, obj_cut_sd_2);
    } else if (sstate == 3) {
        scr_save_room_state(name, 3);
        with (obj_player) {
            scr_costume_black();
            skeleton_attachment_set("head", "char black/head_bearded");
            scr_set_weapon(obj_greatsword);
            x            = 3350;
            image_xscale = 1;
        }
        with (obj_door) {
            if (index == 2)
                instance_destroy();
        }
    }
}
</string>
