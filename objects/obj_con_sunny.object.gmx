          <argument>
            <kind>1</kind>
            <string>///adjust background layers

event_inherited();

background_x[3] = view_xview[0] + cloud_x;
background_x[2] = view_xview[0] + cloud_x / 4;
background_x[4] = view_xview[0] / 3;
background_x[5] = view_xview[0] / 5;
background_x[6] = view_xview[0] / 7;
//background_x[7] = -view_xview[0] * 1.1;
cloud_x         = (cloud_x + 2)%100000;


if (music_enable) {
    if intense {
        if (!audio_is_playing(music_highintense)) {
            audio_stop_sound(level_music);
            level_music = scr_play_music(music_highintense);
        }
    } else {
        if (level_music = 0)
            level_music = scr_play_music(music_lowintense);
        if (level_music)
        if instance_exists(obj_bleed)
            intense = true;
    }
}





with (obj_soldier) {
    if (!scr_is_dead()) {
        var t = id;
        if (x &gt; view_xview[0] + 400) {
            if (x &lt; view_xview[0] + view_wview[0] - 400) {
                if target = 0
                    target = obj_player;
                if obj_player.target = 0
                    obj_player.target = t;
            }
        }
    }
}


scr_check_if_died();
</string>
