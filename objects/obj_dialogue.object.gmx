            <string>///draw box and text
scr_open_box(xx, yy, x_lim, yy1);
if (opened &amp;&amp; start = 0) {
    current_page = 1;
    start = 1;
}
if (current_page != 0) {
    draw_set_font(font_sample);
    draw_set_halign(fa_right);
    draw_set_color(c_black);
    draw_set_alpha(alpha);
    alpha += 0.03;
    if alpha &gt; 1
        alpha = 1;
    var t = 0;
    for (t = 0; t &lt; j; t ++) {
        draw_text(xx + 10 * x_lim - 9, yy + t * 40, page_line[current_page, t]);
    }
    var str_orig = page_line[current_page, j];
    var str = string_delete(str_orig, string_length(str_orig) - i + 0.5, -i);
    var ss = string_length(str_orig);
    if i &lt;= ss {
        i = i + 0.5;
        if i &lt; ss * 0.5
            scr_play_sound(snd_dialogue);
    }
    else {
        if j &lt; num_of_lines[current_page] - 1 {
            j++;
            i = 0;
        }
    }
    
    draw_text(xx + 10 * x_lim - 9, yy + t * 40, str);
    draw_set_alpha(1);
}
